    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5d6090"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        },
        {
            "scope": [
                "variable.other.predefined",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        },
        {
            "scope": [
                "variable.other.constant.object",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "punctuation.definition.template-expression",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#a8eff0"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#e6f0fa",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#b5d6fb"
            }
        },
        {
            "scope": [
                "storage",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#b5d6fb"
            }
        },
        {
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#a6accdCC"
            }
        },
        {
            "scope": [
                "support.class",
                "support.type",
                "support.variable",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self"
            ],
            "settings": {
                "foreground": "#a6accdCC"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#83e1c8"
            }
        },
        {
            "scope": [
                "punctuation.definition.tag",
                "punctuation.definition.typeparameters"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#b5d6fb"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#a8eff0"
            }
        },
        {
            "scope": [
                "string",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#83e1c8"
            }
        },
        {
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#5ddabf"
            }
        },
        {
            "scope": [
                "support.constant",
                "constant.language"
            ],
            "settings": {
                "foreground": "#83e1c8"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#83e1c8"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.control.new"
            ],
            "settings": {
                "foreground": "#83e1c8"
            }
        },
        {
            "scope": [
                "support.class.error",
                "keyword.control.trycatch",
                "keyword.operator.expression.delete",
                "keyword.operator.expression.void",
                "keyword.operator.void",
                "keyword.operator.delete",
                "constant.language.null",
                "constant.language.boolean.false",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#eb91aa"
            }
        },
        {
            "scope": [
                "meta.brace",
                "punctuation"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        },
        {
            "scope": [
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        },
        {
            "scope": [
                "variable.other.readwrite.alias",
                "entity.name.namespace",
                "storage.modifier.import",
                "storage.modifier.package"
            ],
            "settings": {
                "foreground": "#e6f0fa"
            }
        }
    ],
    "semanticHighlighting": true
}