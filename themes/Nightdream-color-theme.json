    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5d6090"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e4f0fb"
            }
        },
        {
            "scope": [
                "support.variable",
                "variable.other.predefined",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#e4f0fb"
            }
        },
        {
            "scope": [
                "variable.other.constant.object",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#c2e4ff"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "punctuation.definition.template-expression",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#bcd3ff"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#e4f0fb"
            }
        },
        {
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#89cbde"
            }
        },
        {
            "scope": [
                "storage.type",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#7a95bc"
            }
        },
        {
            "scope": [
                "storage.modifier.async",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#89cbde"
            }
        },
        {
            "scope": [
                "support.type.primitive",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#909caa"
            }
        },
        {
            "scope": [
                "support.class",
                "support.type",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self"
            ],
            "settings": {
                "foreground": "#909caa"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#4beec9"
            }
        },
        {
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "foreground": "#a6b6cf"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#7a95bc"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#b2f1ff"
            }
        },
        {
            "scope": [
                "string",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#5de4c7"
            }
        },
        {
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#5ddabf"
            }
        },
        {
            "scope": [
                "support.constant",
                "constant.language"
            ],
            "settings": {
                "foreground": "#5de4c7"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#5de4c7"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.control.new"
            ],
            "settings": {
                "foreground": "#5de4c7"
            }
        },
        {
            "scope": [
                "support.class.error",
                "keyword.control.trycatch",
                "keyword.operator.expression.delete",
                "keyword.operator.expression.void",
                "keyword.operator.void",
                "keyword.operator.delete",
                "constant.language.null",
                "constant.language.boolean.false",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#eb91aa"
            }
        },
        {
            "scope": [
                "keyword.operator.assignment",
                "punctuation"
            ],
            "settings": {
                "foreground": "#a6b6cf"
            }
        },
        {
            "scope": [
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#e2e4ec"
            }
        },
        {
            "scope": [
                "variable.other.readwrite.alias",
                "entity.name.namespace",
                "storage.modifier.import",
                "storage.modifier.package"
            ],
            "settings": {
                "foreground": "#e4f0fb"
            }
        }
    ],
    "semanticHighlighting": true
}