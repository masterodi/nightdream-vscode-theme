    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#7b7fb7"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#ced3ff"
            }
        },
        {
            "name": "Variable other property",
            "scope": [
                "support.variable",
                "variable.other.predefined",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#e4f0fb"
            }
        },
        {
            "name": "Variable other constant object",
            "scope": [
                "variable.other.constant.object"
            ],
            "settings": {
                "foreground": ""
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call.generic",
                "support.function.magic",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#95bcff"
            }
        },
        {
            "name": "Function parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#c3c7f3"
            }
        },
        {
            "name": "Keyword | Keyword operator",
            "scope": [
                "keyword",
                "meta.method.declaration storage.type"
            ],
            "settings": {
                "foreground": "#8488f1"
            }
        },
        {
            "name": "Storage (let, const, async, function, class, extends, ...) | Variable language | Keyword operator expression | Keyword operator new",
            "scope": [
                "storage",
                "variable.language",
                "keyword.operator.expression",
                "keyword.operator.new",
                "keyword.function"
            ],
            "settings": {
                "foreground": "#8b9dd9"
            }
        },
        {
            "name": "Primitive",
            "scope": [
                "support.type.primitive",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#91d7e3"
            }
        },
        {
            "name": "Class",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "support.type"
            ],
            "settings": {
                "foreground": "#91d7e3"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self",
                "variable.language.special.self",
                "entity.name.tag.reference"
            ],
            "settings": {
                "foreground": "#91d7e3"
            }
        },
        {
            "name": "Punctuation tag",
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "foreground": "#e5faff"
            }
        },
        {
            "name": "Attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#abc1ff"
            }
        },
        {
            "name": "Id",
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#98efff"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#69e4c9"
            }
        },
        {
            "name": "String regexp",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#bfffd2"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "support.constant",
                "constant.language",
                "support.type.builtin"
            ],
            "settings": {
                "foreground": "#a7f0bd"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#a7f0bd"
            }
        },
        {
            "name": "Falsy",
            "scope": [
                "support.class.error",
                "keyword.control.trycatch",
                "keyword.operator.expression.delete",
                "keyword.operator.expression.void",
                "keyword.operator.void",
                "keyword.operator.delete",
                "constant.language.null",
                "constant.language.boolean.false",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#eb91aa"
            }
        },
        {
            "name": "Keyword operator",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "punctuation.accessor",
                "punctuation.separator.key-value",
                "punctuation.definition.block.sequence.item",
                "punctuation.separator.dictionary.key-value",
                "punctuation.definition.variable",
                "punctuation.separator.colon",
                "punctuation.separator.period",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#b3ceff"
            }
        },
        {
            "name": "CSS property name",
            "scope": [
                "source.css support.type.property-name"
            ],
            "settings": {
                "foreground": "#eb91aa"
            }
        },
        {
            "name": "Variable other readwrite alias",
            "scope": [
                "variable.other.readwrite.alias",
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#add7ff"
            }
        },
        {
            "name": "Json key, Yaml key",
            "scope": [
                "support.type.property-name.json",
                "source.yaml entity.name.tag"
            ],
            "settings": {
                "foreground": "#add7ff"
            }
        }
    ],
    "semanticHighlighting": true
}