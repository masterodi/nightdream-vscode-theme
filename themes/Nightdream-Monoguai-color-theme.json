    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#696D77"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#eaf2f1"
            }
        },
        {
            "scope": [
                "variable.other.predefined",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#eaf2f1"
            }
        },
        {
            "scope": [
                "variable.other.constant.object",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#a99dec"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "punctuation.definition.template-expression",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#bad761"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ffaf7f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#FF657A"
            }
        },
        {
            "scope": [
                "storage",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#9CD1BB"
            }
        },
        {
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#9cb4d1"
            }
        },
        {
            "scope": [
                "support.class",
                "support.type",
                "support.variable",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self"
            ],
            "settings": {
                "foreground": "#9cb4d1"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#FF657A"
            }
        },
        {
            "scope": [
                "punctuation.definition.tag",
                "punctuation.definition.typeparameters"
            ],
            "settings": {
                "foreground": "#888D94"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#9CD1BB"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#9cb4d1"
            }
        },
        {
            "scope": [
                "string",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#FFD76D"
            }
        },
        {
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#FFD76D"
            }
        },
        {
            "scope": [
                "support.constant",
                "constant.language"
            ],
            "settings": {
                "foreground": "#a99dec"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#a99dec"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.control.new"
            ],
            "settings": {
                "foreground": "#9CD1BB"
            }
        },
        {
            "scope": [
                "support.class.error",
                "keyword.control.trycatch",
                "keyword.operator.expression.delete",
                "keyword.operator.expression.void",
                "keyword.operator.void",
                "keyword.operator.delete",
                "constant.language.null",
                "constant.language.boolean.false",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#FF657A"
            }
        },
        {
            "scope": [
                "meta.brace",
                "punctuation"
            ],
            "settings": {
                "foreground": "#888D94"
            }
        },
        {
            "scope": [
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#EAF2F1"
            }
        },
        {
            "scope": [
                "variable.other.readwrite.alias",
                "entity.name.namespace",
                "storage.modifier.import",
                "storage.modifier.package"
            ],
            "settings": {
                "foreground": "#eaf2f1"
            }
        }
    ],
    "semanticHighlighting": true
}